  "editor.formatOnSave": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/__pycache__": true,
    "**/.env.python": true,
    "**/.gitignore": true,
    "**/.devcontainer": true
  },
  "explorerExclude.backup": null,
  "python.formatting.provider": "autopep8"
}
